        <definition>=#&quot;SYSTEM_SYSRULES_dashboardLayout&quot;(
  firstColumnContents: {

    #&quot;SYSTEM_SYSRULES_textField&quot;(
      label: &quot;First name&quot;,
      labelPosition: &quot;ADJACENT&quot;,
      value: rf!first_name,
      readonly: true
    ),
    #&quot;SYSTEM_SYSRULES_textField&quot;(
      label: &quot;Last name&quot;,
      labelPosition: &quot;ADJACENT&quot;,
      value: rf!last_name,
      refreshAfter: &quot;UNFOCUS&quot;,
      readonly: true
    ),
    #&quot;SYSTEM_SYSRULES_textField&quot;(
      label: &quot;Email&quot;,
      labelPosition: &quot;ADJACENT&quot;,
      value: rf!email,
      refreshAfter: &quot;UNFOCUS&quot;,
      readonly: true
    )
    ,
    #&quot;SYSTEM_SYSRULES_textField&quot;(
      label: &quot;Phone&quot;,
      labelPosition: &quot;ADJACENT&quot;,
      value: rf!phone,
      refreshAfter: &quot;UNFOCUS&quot;,
      readonly: true
    )
,
    #&quot;SYSTEM_SYSRULES_dateField&quot;(
      label: &quot;Date of birth&quot;,
      labelPosition: &quot;ADJACENT&quot;,
      value: rf!date_of_birth,
      readonly: true
    )
,
  
        #&quot;SYSTEM_SYSRULES_textField&quot;(
      label: &quot;Team&quot;,
      labelPosition: &quot;ADJACENT&quot;,
      value: rf!team,
      readonly: true
    )
  }
)</definition>
